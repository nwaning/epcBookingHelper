<h1>Listing availabilities</h1>

<table>
  <tr>
    <th>Street</th>
    <th>Nr</th>
    <th>Region</th>
    <th>Fttc</th>
    <th>Ftth</th>
    <th>Wimax</th>
    <th>Richtfunk</th>
    <th>Istgebiet</th>
    <th>Plz</th>
    <th>Ort</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @availabilities.each do |availability| %>
  <tr>
    <td><%= availability.street %></td>
    <td><%= availability.nr %></td>
    <td><%= availability.region_id %></td>
    <td><%= availability.FTTC %></td>
    <td><%= availability.FTTH %></td>
    <td><%= availability.WiMax %></td>
    <td><%= availability.Richtfunk %></td>
    <td><%= availability.istGebiet %></td>
    <td><%= availability.plz %></td>
    <td><%= availability.ort %></td>
    <td><%= link_to 'Show', availability %></td>
    <td><%= link_to 'Edit', edit_availability_path(availability) %></td>
    <td><%= link_to 'Destroy', availability, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Availability', new_availability_path %>
